        <th>Prioridad</th>
        <th>Departamento</th>
        <th>Fecha de Incidencia</th>
        <th>Tecnico</th>
    </tr>
    <% incidencias.forEach(incidencia=> { %>
        <tr>
            <td>
                <%= incidencia.id %>
            </td>
            <td>
                <%= incidencia.descripcio %>
            </td>
            <td>
                <%= incidencia.prioritat ? incidencia.prioritat : 'Sense prioritat'%>
            </td>
            <td>
                <%= incidencia.Departamento ? incidencia.Departamento.nombre : 'Sin departamento' %>
            </td>
            <td>
                <%= incidencia.dataincidencia %>
            </td>
            <td>
                <%= incidencia.Tecnico ? incidencia.Tecnico.nombre : 'Sin Tecnico' %>
            </td>
        </tr>
        <% }); %>
</table>