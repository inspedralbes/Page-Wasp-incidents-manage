<table class="table">
    <tr>
        <th>ID</th>
        <th>Descripción</th>
        <th>Prioridad</th>
        <th>Departamento</th>
        <th>Fecha de Incidencia</th>
        <th>Tecnico</th>
        <th>Accions</th>
    </tr>
    <% incidencias.forEach(incidencia => { %>
        <tr>
            <td><%= incidencia.id %></td>
            <td><%= incidencia.descripcio %></td>
            <td><%= incidencia.prioritat ? incidencia.prioritat : 'Sense prioritat' %></td>
            <td><%= incidencia.Departamento ? incidencia.Departamento.nombre : 'Sense departamento' %></td>
            <td><%= incidencia.dataincidencia %></td>
            <td><%= incidencia.Tecnico ? incidencia.Tecnico.nombre : 'Sense Tècnic' %></td>
            <td>
                <a href="/incidencias/list/<%= incidencia.id %>/editar" class="btn btn-sm btn-secondary">Editar</a>
            </td>
        </tr>
    <% }); %>
</table>
